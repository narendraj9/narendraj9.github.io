<mvc:View
 xmlns="sap.m"
 xmlns:mvc="sap.ui.core.mvc"
 xmlns:core="sap.ui.core"
 controllerName="cart.ProductsList">
 <SelectList
  headerText=""
  id="selectList"
  class="sapUiResponsiveMargin"
  width="auto"
  selectionChange="onSelectionChange"
  items="{/ProductCollection}">
  <core:Item
   key="{ProductId}"
   text="{Name}" />
   
 </SelectList>
</mvc:View>




